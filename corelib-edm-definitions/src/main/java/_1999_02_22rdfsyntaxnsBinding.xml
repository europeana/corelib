<binding xmlns:edm="http://www.europeana.eu/schemas/edm/" xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" trim-whitespace="true">
  <namespace uri="http://www.w3.org/1999/02/22-rdf-syntax-ns#" default="elements" prefix="rdf"/>
  <namespace uri="http://www.w3.org/XML/1998/namespace" prefix="xml"/>
  <mapping class="eu.europeana.corelib.definitions.jibx.RDF" name="RDF">
    <structure map-as="edm:RDF"/>
  </mapping>
  <mapping class="eu.europeana.corelib.definitions.jibx.Type1" name="type">
    <structure map-as="rdf:ResourceType"/>
  </mapping>
  <mapping abstract="true" type-name="rdf:ResourceType" class="eu.europeana.corelib.definitions.jibx.ResourceType">
    <value style="attribute" name="resource" ns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" get-method="getResource" set-method="setResource" usage="optional"/>
  </mapping>
  <mapping abstract="true" type-name="rdf:DateType" class="eu.europeana.corelib.definitions.jibx.DateType">
    <value style="text" get-method="getDate" set-method="setDate"/>
    <value style="attribute" name="datatype" ns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" get-method="getDatatype" set-method="setDatatype" usage="optional"/>
  </mapping>
  <mapping abstract="true" type-name="rdf:LongType" class="eu.europeana.corelib.definitions.jibx.LongType">
    <value style="text" get-method="getLong" set-method="setLong"/>
    <value style="attribute" name="datatype" ns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" get-method="getDatatype" set-method="setDatatype" usage="optional"/>
  </mapping>
  <mapping abstract="true" type-name="rdf:IntegerType" class="eu.europeana.corelib.definitions.jibx.IntegerType">
    <value style="text" get-method="getLong" set-method="setLong"/>
    <value style="attribute" name="datatype" ns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" get-method="getDatatype" set-method="setDatatype" usage="optional"/>
  </mapping>
  <mapping abstract="true" type-name="rdf:NonNegativeIntegerType" class="eu.europeana.corelib.definitions.jibx.NonNegativeIntegerType">
    <value style="text" get-method="getInteger" set-method="setInteger"/>
    <value style="attribute" name="datatype" ns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" get-method="getDatatype" set-method="setDatatype" usage="optional"/>
  </mapping>
  <mapping abstract="true" type-name="rdf:DoubleType" class="eu.europeana.corelib.definitions.jibx.DoubleType">
    <value style="text" get-method="getDouble" set-method="setDouble"/>
    <value style="attribute" name="datatype" ns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" get-method="getDatatype" set-method="setDatatype" usage="optional"/>
  </mapping>
  <mapping abstract="true" type-name="rdf:StringType" class="eu.europeana.corelib.definitions.jibx.StringType">
    <value style="text" get-method="getString" set-method="setString"/>
    <value style="attribute" name="datatype" ns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" get-method="getDatatype" set-method="setDatatype" usage="optional"/>
  </mapping>
  <mapping abstract="true" type-name="rdf:HexBinaryType" class="eu.europeana.corelib.definitions.jibx.HexBinaryType">
    <value style="text" get-method="getString" set-method="setString"/>
    <value style="attribute" name="datatype" ns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" get-method="getDatatype" set-method="setDatatype" usage="optional"/>
  </mapping>
  <mapping abstract="true" type-name="rdf:AboutType" class="eu.europeana.corelib.definitions.jibx.AboutType">
    <value style="attribute" name="about" ns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" get-method="getAbout" set-method="setAbout" usage="optional"/>
  </mapping>
  <mapping abstract="true" type-name="rdf:LiteralType" class="eu.europeana.corelib.definitions.jibx.LiteralType">
    <value style="text" get-method="getString" set-method="setString"/>
    <structure get-method="getLang" set-method="setLang" usage="optional">
      <value style="attribute" name="lang" ns="http://www.w3.org/XML/1998/namespace" get-method="getLang" set-method="setLang" usage="optional"/>
    </structure>
  </mapping>
  <mapping abstract="true" type-name="rdf:ResourceOrLiteralType" class="eu.europeana.corelib.definitions.jibx.ResourceOrLiteralType">
    <value style="text" get-method="getString" set-method="setString"/>
    <structure get-method="getLang" set-method="setLang" usage="optional">
      <value style="attribute" name="lang" ns="http://www.w3.org/XML/1998/namespace" get-method="getLang" set-method="setLang" usage="optional"/>
    </structure>
    <structure get-method="getResource" set-method="setResource" usage="optional">
      <value style="attribute" name="resource" ns="http://www.w3.org/1999/02/22-rdf-syntax-ns#" get-method="getResource" set-method="setResource" usage="optional"/>
    </structure>
  </mapping>
</binding>