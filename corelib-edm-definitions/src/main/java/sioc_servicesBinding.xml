  <mapping class="eu.europeana.corelib.definitions.jibx.HasService" name="has_service">
    <structure map-as="rdf:ResourceType"/>
  </mapping>
  <mapping class="eu.europeana.corelib.definitions.jibx.Service" name="Service">
    <structure map-as="rdf:AboutType"/>
    <collection item-type="eu.europeana.corelib.definitions.jibx.ConformsTo" get-method="getConformsToList" set-method="setConformsToList" create-type="java.util.ArrayList"/>
    <collection item-type="eu.europeana.corelib.definitions.jibx.Implements" get-method="getImplementList" set-method="setImplementList" usage="optional" create-type="java.util.ArrayList"/>
  </mapping>
</binding>